{
   

  "manifest_version": 3,
  "name": "TubeFace",
	"description": "Enable you to play youtube links inside facebook",
  "version": "1.1.0",
  "author":"Santa Bogdan <bogdansanta@live.com>",
  "content_scripts": [
    {
      "run_at": "document_start",
      "matches": [
        "*://www.facebook.com/*"
      ],
      "js": ["jquery-3.2.1.min.js", "content.js"]
    }
  ],
  "short_name":"MTS"
  
}